{
  "name": "tiralabra",
  "version": "0.0.1",
  "description": "Coursework for Data Sturctures Lab in autumn 2021",
  "main": "index.js",
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "docker-compose up",
    "reset": "docker-compose down -v && docker-compose up --build",
    "start:frontend": "HOST=0.0.0.0 react-scripts --openssl-legacy-provider start",
    "start:backend": "NODE_ENV=production node ./src/backend/index.js",
    "build": "react-scripts build",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "test:coverage": "NODE_ENV=test jest --runInBand --coverage",
    "test:frontend": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "prettier": "npx prettier --write src",
    "prettier:ci": "npx prettier --check src",
    "start:dev": "NODE_ENV=development concurrently \"nodemon ./src/backend/index.js\" \"npm run start:frontend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FinThunderstorm/tiralabra.git"
  },
  "author": "Tuomas Alanen <tualanen@cs.helsinki.fi>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FinThunderstorm/tiralabra/issues"
  },
  "homepage": "https://github.com/FinThunderstorm/tiralabra#readme",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "@pathfinder": "src/pathfinder/algorithm",
    "@datastructures": "src/pathfinder/datastructures"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.11.0",
    "eslint-airbnb-react-prettier": "^0.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "jest": "26.6.0",
    "nodemon": "^2.0.14",
    "prettier": "2.4.1",
    "react-app-rewired": "^2.1.8",
    "supertest": "^6.1.6",
    "webpack": "4.44.2"
  },
  "config-overrides-path": "utils/config.overrides.js"
}
